{
  "name": "sinwan",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/sinwan.js",
  "module": "./dist/sinwan.js",
  "types": "./dist/types/sinwan.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sinwan.d.ts",
      "import": "./dist/sinwan.js",
      "default": "./dist/sinwan.js"
    }
  },
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build ./src/sinwan.ts --outdir ./dist --target bun --minify --sourcemap",
    "build:types": "bun x tsc -p tsconfig.build.json",
    "dev": "bun --watch examples/example.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {}
}
