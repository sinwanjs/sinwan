{
  "name": "sinwan",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build ./src/sinwan.ts --outdir ./dist --target bun --minify --sourcemap",
    "build:types": "bun x tsc -p tsconfig.build.json",
    "dev": "bun --watch examples/example.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/cookie": "^1.0.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/bun": "latest",
    "@types/etag": "^1.8.4",
    "@types/accepts": "^1.3.7"
  },
  "dependencies": {
    "accepts": "^1.3.8",
    "cookie": "^1.1.1",
    "cookie-signature": "^1.2.2",
    "etag": "^1.8.1",
    "fs-extra": "^11.3.3",
    "mime-types": "^3.0.2"
  }
}
